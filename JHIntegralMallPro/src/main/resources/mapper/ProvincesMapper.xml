<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jh.mircomall.dao.ProvincesDao">
  <resultMap id="BaseResultMap" type="com.jh.mircomall.bean.Provinces">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="provinceid" jdbcType="VARCHAR" property="provinceid" />
    <result column="province" jdbcType="VARCHAR" property="province" />
  </resultMap>
  <select id="listAllProvinces" resultType="com.jh.mircomall.bean.Provinces">
  	select id,provinceid,province from t_provinces
  </select>
</mapper>